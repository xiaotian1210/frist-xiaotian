		</dependency>
		<dependency>
			<groupId>org.springframework.boot</groupId>
			<artifactId>spring-boot-devtools</artifactId>
			<optional>true</optional>
		</dependency>
		<dependency>
			<groupId>com.shareworx</groupId>
			<artifactId>Platform-api</artifactId>
			<version>${shareworx.version}</version>
		</dependency>
		<dependency>
			<groupId>com.shareworx</groupId>
			<artifactId>Platform-metadata</artifactId>
			<version>${shareworx.version}</version>
		</dependency>
		<dependency>
			<groupId>com.shareworx</groupId>
			<artifactId>Platform-webmvc</artifactId>
			<version>${shareworx.version}</version>
		</dependency>
		<!-- 邮件发送引用模块 -->
		<dependency>
			<groupId>com.shareworx</groupId>
			<artifactId>Platform-mail</artifactId>
			<version>${shareworx.version}</version>
		</dependency>
		 <dependency> <groupId>com.shareworx</groupId> <artifactId>Platform-cache-redis</artifactId> 
			<version>${shareworx.version}</version> </dependency> 
		<dependency>
			<groupId>org.apache.poi</groupId>
			<artifactId>poi-ooxml</artifactId>
			<version>3.9</version>
		</dependency>
		<!-- 新增 sqlite 依赖 -->
		<dependency>
			<groupId>org.xerial</groupId>
			<artifactId>sqlite-jdbc</artifactId>
			<version>3.8.7</version>
		</dependency>
		 <dependency> <groupId>com.shareworx</groupId> <artifactId>Platform-admin</artifactId> 
			<version>${shareworx.version}</version> </dependency> 
		<dependency>
			<groupId>org.apache.cxf</groupId>
