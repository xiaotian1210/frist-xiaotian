<!DOCTYPE html>
<html>
	<head>
		<base href="${basePath?if_exists}">
		<meta charset="UTF-8">
		<link rel="stylesheet" type="text/css" href="resources/ezfm/all.css"></link>
		<link rel="stylesheet" type="text/css" href="resources/ezfm/css/baseinfo/baseinfo.css"></link>
		<script type="text/javascript" src="resources/ezfm/all.js"></script>
		<script type="text/javascript" src="resources/ezfm/js/system/precrop.js"></script>
		<script type="text/javascript">
		var notRoleButtons = "${notRoleButtons?if_exists}";
		</script>
	</head>
	<body class="easyui-layout">
	
		<div id="tb" style="height:auto">
			<div style="margin-bottom:5px;margin-top:5px">
				<a id="btnCheck" href="ezfm/system/crop/index" class="easyui-linkbutton" data-options="iconCls:'icon-more',plain:true">管理</a>
				<span style="float:right;margin-bottom:5px;margin-top:5px;margin-right:20px;">
					<input id="querysearch" class="easyui-searchbox" style="width:300px" data-options="menu:'#mm'"></input>
				</span>
				<div id="mm" style="width:120px"> 
					<div data-options="name:'name_'">企业名称</div> 
					<div data-options="name:'code_'">企业编码</div> 
				</div>
			</div>
		</div>
		
		<div data-options="region:'center'">	
			<table id="datagrid" class="easyui-datagrid" style="width:700px;height:250px" data-options="fit:true,title:'企业审核',singleSelect:true,toolbar:'#tb'">
				<thead>
					<tr>
						<th data-options="field:'pk_pre_crop',checkbox:true"></th>
						<th data-options="field:'code',align:'center',halign:'center',width:160">企业编码</th>
						<th data-options="field:'name',align:'center',halign:'center',width:160">企业名称</th>
						<th data-options="field:'contact',align:'center',width:120">联系人</th>
						<th data-options="field:'phone',align:'center',width:160">联系电话</th>
						<th data-options="field:'address',align:'left',halign:'center',width:280">联系地址</th>
						<th data-options="field:'post_code',align:'center',width:120">邮编</th>
						<th data-options="field:'email',align:'center',width:180">邮箱</th>
						<th data-options="field:'apply_time',align:'center',width:120">申请时间</th>
						<th data-options="field:'check_time',align:'center',width:120">审核时间</th>
						<th data-options="field:'check_user',align:'center',width:120">审核人员 </th>
						<!--  <th data-options="field:'check_state',align:'center',width:120">审核状态</th>-->
						<th data-options="field:'file',align:'center',width:100,formatter:formatFile">企业执照</th>
						<th data-options="field:'check_state',align:'center',width:240,formatter:formatOperation">操作</th>
						
					</tr>
				</thead>
			</table>
		</div>
	</body>
</html>